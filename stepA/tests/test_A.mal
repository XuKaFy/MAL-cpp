(define create-accumulator (lambda ()
    (define value 0)
    (define accumulator (lambda (delta) 
        (set value (+ value delta))
        value
    ))
    accumulator
))

(define a (create-accumulator))
(define b (create-accumulator))

(println (a 1))
(println (a 3))
(println (b 2))

